{
    "name": "<%= name %>",
    "version": "<%= version %>",
    "private": true,
    "author": {
        "name": "<%= author %>",
        "email": "<%= email %>"
    },
    "repository": {
        "type": "git",
        "url": "http://gitlab.xxx.com/groups/<%= groupName %>/<%= name %>"
    },
    "group": "<%= groupName %>",
    "type": "<%= frameType %>",
    "keywords": [
        "earth",
        "react",
        "antd4",
        "ts",
        "<%= name %>"
    ],
    "dependencies": {
        "antd": "4.0.2",
        "react": "16.13.0",
        "react-dom": "16.13.0",
        "redux-saga": "1.0.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.144",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.1.2",
        "@types/react-router-dom": "^5.1.3",
        "earth-scripts": "^3.0.0"
    },
    "scripts": {
        "start": "earth-scripts start",
        "build": "earth-scripts build",
        "update": "earth-scripts update",
        "test": "earth-scripts test --env=jsdom",
        "mock": "earth-scripts mock"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "jsx-a11y/anchor-is-valid": 0
        }
    },
    "mockRoot": "mock_server.js",
    "proxy=>moved-to-/src/setupProxy.js": "http://127.0.0.1:8004"
}
